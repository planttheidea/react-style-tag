{
  "author": "planttheidea",
  "ava": {
    "babel": "inherit",
    "concurrency": 5,
    "failFast": true,
    "files": ["test/*.js", "!test/helpers/*.js"],
    "require": ["babel-register", "babel-polyfill", "./test/helpers/setup-browser-env.js"],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/planttheidea/react-style-tag/issues"
  },
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "blob-polyfill": "^1.0.20150320",
    "lodash": "^4.16.4",
    "postcss": "^6.0.14",
    "prop-types": "^15.5.7",
    "uuid": "^3.1.0"
  },
  "description": "Write scoped, autoprefixed styles declaratively in React",
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-env": "^3.2.2",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.1",
    "eslint": "^4.10.0",
    "eslint-config-rapid7": "^2.11.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "html-webpack-plugin": "^2.30.1",
    "in-publish": "2.0.0",
    "json-loader": "0.5.7",
    "nyc": "^11.3.0",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "sinon": "^4.1.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "homepage": "https://github.com/planttheidea/react-style-tag#readme",
  "keywords": ["react", "style", "css", "css-modules", "postcss"],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "react-style-tag",
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0",
    "react-dom": "^15.3.0 || ^16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/react-style-tag.git"
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --progress --colors --config=webpack.config.dev.js",
    "lint": "NODE_ENV=test eslint src",
    "prepublish": "in-publish && npm run prepublish:compile || echo",
    "prepublish:compile": "npm run lint && npm run test:coverage && npm run transpile",
    "start": "npm run dev",
    "test": "NODE_PATH=. NODE_ENV=test ava --no-cache",
    "test:coverage": "nyc npm test",
    "test:watch": "NODE_PATH=. NODE_ENV=test ava --watch",
    "test:update": "NODE_PATH=. NODE_ENV=test ava --update-snapshots",
    "transpile": "babel src --out-dir lib"
  },
  "version": "1.4.0"
}
